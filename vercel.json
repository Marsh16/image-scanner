{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {"runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/main"
        }
    ]
}
